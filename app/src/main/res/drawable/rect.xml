<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="-20dp"
        android:left="-20dp"
        android:right="-20dp"
        android:top="-20dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="@dimen/rect_stroke_width"
                android:color="@color/rect_stroke" />
            <corners android:radius="@dimen/rect_corners_radius" />
            <solid android:color="@color/rect_transparent" />
        </shape>

    </item>
</layer-list>